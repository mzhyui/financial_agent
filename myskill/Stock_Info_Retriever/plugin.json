{
  "name": "Stock_Info_Retriever",
  "version": "1.0.4",
  "description": "Retrieves stock information based on user queries.",
  "author": null,
  "license": "MIT",
  "active": false,
  "hubId": "stock-info-retriever",
  "entrypoint": {
    "file": "handler.js",
    "params": {
      "symbol": {
        "type": "string",
        "required": true,
        "input": {
          "type": "text",
          "default": "AAPL",
          "placeholder": "AAPL",
          "hint": "The stock symbol to retrieve information for"
        },
        "value": "AAPL"
      },
      "start_date": {
        "type": "string",
        "required": false,
        "input": {
          "type": "date",
          "default": "",
          "placeholder": "YYYY-MM-DD",
          "hint": "The start date for historical data (optional)"
        },
        "value": ""
      },
      "end_date":{
        "type": "string",
        "required": false,
        "input": {
          "type": "date",
          "default": "",
          "placeholder": "YYYY-MM-DD",
          "hint": "The end date for historical data (optional)"
        },
        "value": ""
      }
    }
  },
  "setup_args": {
    "ALPHAVANTAGE_API_KEY": {
      "type": "string",
      "required": true, 
      "input": {
        "type": "text", 
        "default": "YOUR_ALPHAVANTAGE_API_KEY",
        "placeholder": "4MFYSPOPV210V5WT",
        "hint": "The API key for the ALPHAVANTAGE API"
      },
      "value": "4MFYSPOPV210V5WT"
    },
    "LOCALSRVER_CSV_URL": {
      "type": "string",
      "required": false,
      "input": {
        "type": "text",
        "default": "",
        "placeholder": "",
        "hint": "The URL of the local server providing CSV data, e.g., http://localhost:8081/"
      },
      "value": ""
    }
  },
  "examples": [
    {
      "prompt": "Retrieve historical data for AAPL from 2023-01-01 to 2023-12-31",
      "call": "{\"symbol\": \"AAPL\", \"start_date\": \"2023-01-01\", \"end_date\": \"2023-12-31\"}"
    },
    {
      "prompt": "What is the stock price of TSLA?",
      "call": "{\"symbol\": \"TSLA\"}"
    }
  ]
}
